{
  "name": "hoteles",
  "version": "1.0.0",
  "description": "Pagina web de Hoteles",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\" \"npm run dev\"",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d -dist --htmlmin -o dist/index.html && usemin condiciones.html -d -dist --htmlmin -o dist/condiciones.html && usemin about.html -d -dist --htmlmin -o dist/about.html && usemin contacto.html -d -dist --htmlmin -o dist/contacto.html && usemin precios.html -d -dist --htmlmin -o dist/precios.html",
    "build": "npm run clean && npm run scss && npm run imagemin && npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imoreno73/Hoteles.git"
  },
  "author": "Ignacio Moreno Frison",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/imoreno73/Hoteles/issues"
  },
  "homepage": "https://github.com/imoreno73/Hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "grunt": "^1.6.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-sass": "^2.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "lite-server": "^2.6.1",
    "node-sass": "^8.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^4.1.2"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.7",
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.3",
    "concurrently": "^7.6.0",
    "cssmin": "^0.4.3",
    "htmlmin": "^0.0.7",
    "imagemin": "^8.0.1",
    "jquery": "^3.6.3",
    "jquery.min.js": "^3.5.1",
    "open-iconic": "^1.1.1",
    "popper": "^1.0.1",
    "popper.js": "^1.16.1",
    "ua-parser-js": "^1.0.33",
    "uglify-js": "^3.17.4",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  }
}
